<div class="container">
<h3>Project Name : <%= @project.projectname %></h3>
<h3>Title : <%= @project.title %></h3>
<h3>Department : <%= @department.departmentname %></h3>
<%= link_to "Edit", edit_project_path(@project), :class => 'btn btn-outline-primary' %>
<%= link_to "Delete", project_path(@project), method: :delete, data: {confirm: 'Are You Sure?'}, :class => 'btn btn-outline-danger' %>
</div>
<hr>
<% @users.each do |user| %>
<div class="card body">
  <h3> User Name : <%= user.name %></h3>
</div>
<br/>
<% end %>
